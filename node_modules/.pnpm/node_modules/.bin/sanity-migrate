#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/migrate/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/migrate/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/migrate/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/migrate/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+migrate@5.2.3_@noble+hashes@2.0.1_@types+node@25.1.0_@types+react@19.2.10_jiti@_d392aac556f631370e21eda7d5893573/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/migrate/bin/run.js" "$@"
else
  exec node  "$basedir/../@sanity/migrate/bin/run.js" "$@"
fi
