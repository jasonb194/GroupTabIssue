#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/export/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/export/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/export/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/export/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+export@6.0.5/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@sanity+export@6.0.5/node_modules/@sanity/export/bin/detect-corrupt.js" "$@"
else
  exec node  "$basedir/../../../../../@sanity+export@6.0.5/node_modules/@sanity/export/bin/detect-corrupt.js" "$@"
fi
