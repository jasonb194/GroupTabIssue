#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/cli/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/cli/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/cli/bin/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/cli/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules/@sanity/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/@sanity+cli@5.7.0_@types+node@25.1.0_babel-plugin-react-compiler@1.0.0_react@19.2.4_tsx_107753d04f19d4d7f6669c23ea9a72cd/node_modules:/Users/jason.brown/SanityProjects/Customer/bugtest/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sanity" "$@"
else
  exec node  "$basedir/../../bin/sanity" "$@"
fi
